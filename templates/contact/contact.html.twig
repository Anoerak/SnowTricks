{% extends "base.html.twig" %} {% block title %}SnowTricks - Contact{% endblock %} {% block body %}
<section id="contact">
	<h2>Contact Us</h2>
	{{ form_start(form) }}
	<div id="form">
		{{ form_widget(form) }}
		<button type="submit" class="btn btn-primary">Send</button>
		<button type="reset" class="btn btn-cancel">Cancel</button>
	</div>
	{{ form_end(form) }}
</section>

{% endblock %}
